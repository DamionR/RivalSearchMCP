{
  "$schema": "https://gofastmcp.com/schemas/fastmcp_config/v1.json",
  "entrypoint": {
    "file": "src/server.py",
    "object": "mcp"
  },
  "environment": {
    "dependencies": [
      "fastmcp>=2.11.4",
      "httpx>=0.25.0",
      "cloudscraper>=1.2.0",
      "beautifulsoup4>=4.12.0",
      "lxml>=4.9.0",
      "pillow>=10.0.0",
      "websockets>=11.0.0",
      "pytesseract>=0.3.0",
      "fake-useragent>=1.4.0",
      "selenium>=4.15.0"
